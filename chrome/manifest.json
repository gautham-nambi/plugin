{
    "manifest_version": 2,
    "name": "CSSPath Helper",
    "version": "2.0.2",
    "description": "Extract, edit, and evaluate CSSPath queries with ease.",
    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_icon": {
            "19": "static/out_19x19.png",
            "38": "static/out_38x38.png"
        }
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "css": ["content.css"],
        "js": ["jquery.js", "content.js"]
    }],
    "permissions": ["http://www.google.com/"],
    "icons": {
        "16": "static/out_16x16.png",
        "32": "static/out_32x32.png",
        "48": "static/out_48x48.png",
        "128": "static/out_128x128.png"
    },
    "web_accessible_resources": ["bar.css", "bar.html", "bar.js", "frame.html","jquery.js"]
}